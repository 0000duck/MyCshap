<Window x:Class="InterfaceAndEvent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfaceAndEvent"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Frame x:Name="frame" Grid.Row="0" Grid.RowSpan="2" Content="Frame" HorizontalAlignment="Stretch" Margin="0" VerticalAlignment="Stretch" NavigationUIVisibility="Hidden" IsHitTestVisible="True" SnapsToDevicePixels="True"/>
    </Grid>
</Window>
